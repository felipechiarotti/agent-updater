{
  "Logging": {
    "LogLevel": {
      "System.Net.Http.HttpClient.IUpdaterClient": "Warning",
      "Microsoft.Hosting": "Warning"
    }
  },
  "Updater": {
    "BaseUrl": "https://api.bitbucket.org",
    "Workspace": "",
    "Repository": "",
    "ApiKey": ""
  },
  "Target": {
    "Path": "bin",
    "Assembly": "pmsproxyagent"
  },
  "WindowsService": {
    "UseAsWindowsService": true,
    "ServiceName": "PmsProxyAgentUpdater",
    "TargetServiceName": "PmsProxyAgent",
    "CheckUpdateInterval": "12:00:00"
  },
  "Backup": {
    "Enabled": true,
    "Path": "backup",
    "Files": [
      "appsettings.json"
    ]
  }
}
